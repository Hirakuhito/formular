<?xml version="1.0" encoding="UTF-8"?>
<robot name="formular_car">

    <!--車体のリンク-->
    <link name="body">
        <inertial>
            <mass value="300.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="body.stl" scale="1.0"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="body.stl" scale="1.0"/>
            </geometry>
        </collision>
    </link>

    <!--* STEERINGS -->
    <!-- FL steering -->
    <link name="steering_fl">
        <inertial>
            <mass value="0.001"/>
            <inertia ixx="1e-6" ixy="0.0" ixz="0.0" iyy="1e-6" iyz="0.0" izz="1e-6"/>
        </inertial>
    </link>

    <link name="steering_fr">
        <inertial>
            <mass value="0.001"/>
            <inertia ixx="1e-6" ixy="0.0" ixz="0.0" iyy="1e-6" iyz="0.0" izz="1e-6"/>
        </inertial>
    </link>

    <link name="steering_rl">
        <inertial>
            <mass value="0.001"/>
            <inertia ixx="1e-6" ixy="0.0" ixz="0.0" iyy="1e-6" iyz="0.0" izz="1e-6"/>
        </inertial>
    </link>

    <link name="steering_rr">
        <inertial>
            <mass value="0.001"/>
            <inertia ixx="1e-6" ixy="0.0" ixz="0.0" iyy="1e-6" iyz="0.0" izz="1e-6"/>
        </inertial>
    </link>

    <!--* WHEELS  -->
    <!-- FLホイール -->
    <link name="wheel_fl">
        <inertial>
            <mass value="10.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="wheel_FL.stl" scale="1.0"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="wheel_FL.stl" scale="1.0"/>
            </geometry>
        </collision>
    </link>

    <link name="wheel_fr">
        <inertial>
            <mass value="10.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="wheel_FR.stl" scale="1.0"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="wheel_FR.stl" scale="1.0"/>
            </geometry>
        </collision>
    </link>
 
    <link name="wheel_rl">
        <inertial>
            <mass value="10.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="wheel_RL.stl" scale="1.0"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="wheel_RL.stl" scale="1.0"/>
            </geometry>
        </collision>
    </link>

    <link name="wheel_rr">
        <inertial>
            <mass value="10.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="wheel_RR.stl" scale="1.0"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="wheel_RR.stl" scale="1.0"/>
            </geometry>
        </collision>
    </link>

    <!--* SUSPENSIONS  -->
    <link name="sus_fl">
        <inertial>
            <mass value="2.0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="sus_arm_FL.stl" scale="1.0"/>
            </geometry>
        </visual>
    </link>

    <link name="sus_fr">
        <inertial>
            <mass value="2.0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="sus_arm_FR.stl" scale="1.0"/>
            </geometry>
        </visual>
    </link>

    <link name="sus_rl">
        <inertial>
            <mass value="2.0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="sus_arm_RL.stl" scale="1.0"/>
            </geometry>
        </visual>
    </link>

    <link name="sus_rr">
        <inertial>
            <mass value="2.0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="sus_arm_RR.stl" scale="1.0"/>
            </geometry>
        </visual>
    </link>


    <!--* STEERING JOINTS  -->
    <!-- FL steering joint -->
    <joint name="steering_fl_joint" type="revolute">
        <origin xyz="1.05 -1.4 0.09" rpy="0.0 0.0 0.0"/>
        <parent link="sus_fl_joint"/>
        <child link="steering_fl"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="-0.5236" upper="0.5236" effort="80" velocity="3.0"/> <!-- if you feel so fast, change velocity->1.5 -->
    </joint>

    <joint name="steering_fr_joint" type="revolute">
        <origin xyz="-1.05 -1.4 0.09" rpy="0.0 0.0 0.0"/>
        <parent link="sus_fr_joint"/>
        <child link="steering_fr"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="-0.5236" upper="0.5236" effort="80" velocity="3.0"/> <!-- if you feel so fast, change velocity->1.5 -->
    </joint>

    <joint name="steering_rl_joint" type="fixed">
        <origin xyz="1.1 1.5 0.12" rpy="0.0 0.0 0.0"/>
        <parent link="sus_rl_joint"/>
        <child link="steering_rl"/>
    </joint>

    <joint name="steering_rr_joint" type="fixed">
        <origin xyz="-1.1 1.5 0.12" rpy="0.0 0.0 0.0"/>
        <parent link="sus_rr_joint"/>
        <child link="steering_rr"/>
    </joint>

    <!--* WHEEL JOINTS  -->
    <!-- wheel rotate join -->
    <joint name="wheel_fl_joint" type="continuous">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="steering_fl"/>
        <child link="wheel_fl"/>
        <axis xyz="1.0 0.0 0.0"/>
    </joint>

    <joint name="wheel_fr_joint" type="continuous">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="steering_fr"/>
        <child link="wheel_fr"/>
        <axis xyz="1.0 0.0 0.0"/>
    </joint>

    <joint name="wheel_rl_joint" type="continuous">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="steering_rl"/>
        <child link="wheel_rl"/>
        <axis xyz="1.0 0.0 0.0"/>
    </joint>

    <joint name="wheel_rr_joint" type="continuous">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="steering_rr"/>
        <child link="wheel_rr"/>
        <axis xyz="1.0 0.0 0.0"/>
    </joint>

    <!--* SUPENSION JOINTS -->
    <joint name="sus_fl_joint" type="prismatic">
        <origin xyz="1.05 -1.4 0.09" rpy="0 0 0"/>
        <parent link="body"/>
        <child link="sus_fl"/>
        <axis xyz="0 0 1"/>  <!-- 上下方向（z軸） -->
        <limit lower="-0.10" upper="0.05" effort="5000" velocity="1.0"/>
    </joint>

    <joint name="sus_fr_joint" type="prismatic">
        <origin xyz="-1.05 -1.4 0.09" rpy="0 0 0"/>
        <parent link="body"/>
        <child link="sus_fr"/>
        <axis xyz="0 0 1"/>  <!-- 上下方向（z軸） -->
        <limit lower="-0.10" upper="0.05" effort="5000" velocity="1.0"/>
    </joint>

    <joint name="sus_rl_joint" type="prismatic">
        <origin xyz="1.05 1.5 0.12" rpy="0 0 0"/>
        <parent link="body"/>
        <child link="sus_rl"/>
        <axis xyz="0 0 1"/>  <!-- 上下方向（z軸） -->
        <limit lower="-0.10" upper="0.05" effort="5000" velocity="1.0"/>
    </joint>

    <joint name="sus_fr_joint" type="prismatic">
        <origin xyz="-1.05 1.5 0.12" rpy="0 0 0"/>
        <parent link="body"/>
        <child link="sus_rr"/>
        <axis xyz="0 0 1"/>  <!-- 上下方向（z軸） -->
        <limit lower="-0.10" upper="0.05" effort="5000" velocity="1.0"/>
    </joint>
</robot>